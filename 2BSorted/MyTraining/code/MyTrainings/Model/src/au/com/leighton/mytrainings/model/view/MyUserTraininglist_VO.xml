<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MyUserTraininglist_VO"
  Version="11.1.1.60.13"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="au.com.leighton.mytrainings.model.view.MyUserTraininglist_VO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="courseId"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery>
    <![CDATA[SELECT 
    MytrainingUsertraining_EO.USERTRAINING_ID, 
    MytrainingUsertraining_EO.COURSE_ID, 
    MytrainingUsertraining_EO.USERNAME, 
    MytrainingUsertraining_EO.ADDED_ON 
FROM 
    MYTRAINING_USERTRAINING MytrainingUsertraining_EO 
WHERE 
    MytrainingUsertraining_EO.USERNAME = :userName AND MytrainingUsertraining_EO.COURSE_ID = :courseId]]>
  </SQLQuery>
  <EntityUsage
    Name="MytrainingUsertraining_EO"
    Entity="au.com.leighton.mytrainings.model.entity.MytrainingUsertraining_EO"/>
  <ViewAttribute
    Name="UsertrainingId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UsertrainingId"
    EntityUsage="MytrainingUsertraining_EO"
    AliasName="USERTRAINING_ID"/>
  <ViewAttribute
    Name="CourseId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CourseId"
    EntityUsage="MytrainingUsertraining_EO"
    AliasName="COURSE_ID"/>
  <ViewAttribute
    Name="Username"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Username"
    EntityUsage="MytrainingUsertraining_EO"
    AliasName="USERNAME"/>
  <ViewAttribute
    Name="AddedOn"
    PrecisionRule="true"
    EntityAttrName="AddedOn"
    EntityUsage="MytrainingUsertraining_EO"
    AliasName="ADDED_ON"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="au.com.leighton.mytrainings.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
